<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flysnail.demoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login"></activity>
        <activity
            android:name=".layout.ConstraintLayout_test"
            android:parentActivityName=".layout.Layout_test" />
        <activity
            android:name=".layout.FrameLayout_test"
            android:parentActivityName=".layout.Layout_test" />
        <activity
            android:name=".layout.Layout_test"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".layout.GridLayout_test"
            android:parentActivityName=".layout.Layout_test" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>